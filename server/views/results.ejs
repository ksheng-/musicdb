<!DOCTYPE html>
<html>
	<head>
	<% include head.ejs %>
	</head>
	
	<body>
		<% include navbar.ejs %>
		<div class="container-fluid">
			<div class="row">
				<div class="col-md-8 col-md-offset-2">
					<h1>Search results for ASDASD</h1>
				</div>
			</div>
			<div class="row">
				<div class="col-md-8 col-md-offset-2">
					<table class="table table-hover">
						<tr>
							<% for (var field in results[0]) {%>
								<th><%= field %></th>
							<% } %>
						</tr>
						<% for (var i=0; i<results.length; i++) {%>
							<tr>
								<% for(var field in results[i]) {%>
									<td><%= results[i][field] %></td>
								<% } %>
							</tr>
						<% } %>
					</table>
				</div>
			</div>
		</div>
	</body>

	<% include scripts.ejs %>

</html>
